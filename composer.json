{
    "name": "minnpost/salesforce-rest-api",
    "description": "Defines an API that enables WordPress to interact with the Salesforce REST API.",
    "type": "wordpress-plugin",
    "license": "GPLv2 or later",
    "authors": [
        {
            "name": "MinnPost",
            "email": "tech@minnpost.com"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/pippinsplugins/wp-logging.git",
            "type": "git"
        },
        {
            "url": "https://github.com/MinnPost/wp-background-processing",
            "type": "git"
        } // use this until and unless the https://github.com/A5hleyRich/wp-background-processing fixes the issue with $batch not being an object
    ],
    "require": {
        "minnpost/wp-background-processing": "dev-master",
        "pippinsplugins/wp-logging": "dev-master"
    },
    "autoload": {
        "classmap": ["vendor/pippinsplugins/wp-logging/", "vendor/minnpost/wp-background-processing/"] // use this until and unless the https://github.com/A5hleyRich/wp-background-processing fixes the issue with $batch not being an object
    }
}
